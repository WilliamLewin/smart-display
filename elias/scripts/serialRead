import serial
import time

serial_gps = serial.Serial('/dev/ttyS0')
serial_gps.baudrate = 9600

end_time = 10
start_time = time.time()
stop = 0
while stop < end_time :
    stop_time = time.time()
    stop = stop_time - start_time

serial_read = serial_gps.inWaiting()
print(serial_gps.read(serial_read))
